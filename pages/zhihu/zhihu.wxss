.startimg{
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 100%;
	height: 100%;
	background: #000;
	z-index: 10000;
}
.img{
	width: 100%;
	height: 100%;
}
.swiper{
    width: 100%;
    height: 450rpx;
}
.item-title{
	width:90%;
	margin:auto;
	margin-top:250rpx;
	color:#fff;
	font-family: "Microsoft YaHei",sans-serif;
}
.wen{
    width: 90%;
    height: 150rpx;
    margin: auto;
    margin-top:20rpx; 
    background: #eee ;
    padding:20rpx;
    padding-left:50rpx;
    box-sizing: border-box;
    box-shadow: 0rpx 0rpx 2rpx 2rpx #999;
}
.wen-left{
	width:100rpx;
	height:100rpx;
	margin-top:5rpx;
	float:left;
}
.wen-right{
	width:75%;
	height:110rpx;
	float:right;
	font-size:30rpx;
	font-family: "Microsoft YaHei",sans-serif;
}
.themes{
    position: fixed;
    top:0;
    bottom:0;
    margin: auto;
    width:180px;
    background:#fff;
    z-index: 99;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-size:border-box;
    padding:40rpx;
    font-size: 16px;
    color:#333;
    transform: translate3d(-100%,0,0);
    transition: transform .8s ease;
}
.themes .t-item{
    line-height: 40px;
    font-family: "微软雅黑";
}
.active{
    transform:translate3d(100%,0,0);
    box-shadow: 0 0 0 1000px rgba(0,0,0,0.3);
}

.circle{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	position: fixed;
	right: 30rpx;
	bottom:30rpx;
	background:#28dd8b;
	z-index: 100;
	font-size: 50px;
	text-align: center;
	line-height: 100rpx;
	color: #fff;
}
